{
  "name": "claude-code-orchestrator",
  "version": "1.0.0",
  "description": "A distributed system for orchestrating multiple Claude Code instances",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "tsx tests/e2e/run-e2e.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
